{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    // "module": "esnext",
    "allowSyntheticDefaultImports": true
  },
  "baseUrl": ".",
  "paths": {
    "@/*": [
      "src/*"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "src/**/*",
    "assets/**/*"
  ]
}